
@{
    ViewData["Title"] = "QuizSelect";
}

<h1>QuizSelect</h1>

<div class="row">
    <div class="card" style="width:350px">
        <img class="card-img-top" src="~/Images/Singinkids.png" alt="Card image">
        <div class="card-body"></div>

          <div class="card-footer">
              <h3 class="card-title">Songs</h3>
              <a href="Index" class="btn btn-primary">Start</a>
          </div>
    </div>

    <div class="card" style="width:350px">
        <img class="card-img-top" src="~/Images/NoteNames.png" alt="Card image">
        <div class="card-body"></div>
        <div class="card-footer">
            <h3 class="card-title">Note Names</h3>
            <a asp-controller="Admin" asp-action="NoteQuiz" class="btn btn-primary">Start</a>
        </div>
    </div>

    <div class="card" style="width:350px">
        <img class="card-img-top" src="~/Images/Ear Training.jpg" alt="Card image">
        <div class="card-body"></div>
        <div class="card-footer">
            <h3 class="card-title">Intervals</h3>
            <a href="Index" class="btn btn-primary">Start</a>
        </div>
    </div>
</div>
